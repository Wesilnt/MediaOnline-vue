{
    "name": "MediaOnline-vue",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve --open",
        "build": "vue-cli-service build --modern",
        "lint": "vue-cli-service lint",
        "test": "vue-cli-service build  --modern --mode=development",
        "test:unit": "vue-cli-service test:unit",
        "precommit": "lint-staged",
        "prettier": "prettier --write ./src/**/**/**/*",
        "eslint-fix": "eslint src/**/**/*.vue --fix",
        "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
    },
    "eslintConfig": {
        "extends": [
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error",
            "vue/require-v-for-key": "warning"
        }
    },
    "prettier": {
        "singleQuote": true,
        "semi": false
    },
    "lint-staged": {
        "**/*.{js,vue,scss,less}": [
            "prettier --wirter",
            "git add"
        ]
    },
    "dependencies": {
        "amfe-flexible": "^2.2.1",
        "es6-promise": "^4.2.5",
        "html2canvas": "^1.0.0-alpha.12",
        "postcss-px-to-viewport": "0.0.3",
        "postcss-theme-variables": "^1.0.0",
        "vant": "^1.3.3",
        "vue": "^2.5.17",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1",
        "webpack-encoding-plugin": "^0.2.1",
        "whatwg-fetch": "^2.0.4"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.0.1",
        "@vue/cli-plugin-eslint": "^3.0.1",
        "@vue/cli-plugin-unit-mocha": "^3.0.1",
        "@vue/cli-service": "^3.0.3",
        "@vue/eslint-config-prettier": "^3.0.3",
        "@vue/test-utils": "^1.0.0-beta.25",
        "babel-plugin-import": "^1.9.1",
        "chai": "^4.1.2",
        "eslint": "^5.5.0",
        "eslint-plugin-prettier": "^2.6.2",
        "husky": "^0.14.3",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "lint-staged": "^7.2.2",
        "node-sass": "^4.9.0",
        "postcss-calc": "^6.0.1",
        "postcss-loader": "^3.0.0",
        "precss": "^3.1.2",
        "prettier-eslint-cli": "^4.7.1",
        "qs": "^6.5.2",
        "sass-loader": "^7.0.1",
        "vue-template-compiler": "^2.5.17"
    }
}
